{
  "name": "my-api-server",
  "version": "1.0.0",
  "description": "Koa API Server",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm src/server.js",
    "dev": "nodemon --exec node --loader ts-node/esm src/server.js",
    "test": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "jest --watch",
    "typeorm": "typeorm-ts-node-esm",
    "migration:generate": "node --loader ts-node/esm ./node_modules/typeorm/cli.js migration:generate -d ./src/config/database.js",
    "migration:run": "NODE_ENV=development node --loader ts-node/esm ./node_modules/typeorm/cli.js migration:run -d ./src/config/database.js",
    "migration:revert": "node --loader ts-node/esm ./node_modules/typeorm/cli.js migration:revert -d src/config/database.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.33.1",
    "@google/generative-ai": "^0.21.0",
    "@koa/cors": "^5.0.0",
    "@koa/router": "^12.0.0",
    "@langchain/anthropic": "^0.3.12",
    "@langchain/core": "^0.3.31",
    "@langchain/google-genai": "^0.1.6",
    "@langchain/openai": "^0.3.17",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "googleapis": "^144.0.0",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-passport": "^6.0.0",
    "koa-session": "^7.0.0",
    "langchain": "^0.3.11",
    "moment": "^2.30.1",
    "node-cache": "^5.1.2",
    "p-queue": "^8.0.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.0",
    "qs": "^6.14.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@types/node": "^22.10.7",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "packageManager": "pnpm@10.0.0+sha512.b8fef5494bd3fe4cbd4edabd0745df2ee5be3e4b0b8b08fa643aa3e4c6702ccc0f00d68fa8a8c9858a735a0032485a44990ed2810526c875e416f001b17df12b"
}
